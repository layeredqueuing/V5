G "ERR_JOIN_BAD_PATH" 	# Model comment 
1e-05			# Convergence test value.
50			# Maximum number of iterations.
10			# Print intermediate results (see manual pages)
0.9			# Model under-relaxation ( 0.0 < x <= 1.0)
-1

P 2
  p c0 i
  p p0 s 0.1
-1

T 2
  t c0 r c0 -1 c0
  t t0 f e0 -1 p0
-1

E 2
# ---------- c0 ----------
  s c0 1 -1
  y c0 e0 1 -1
# ---------- t0 ----------
  A e0 a1
-1

A t0
  s a1 0.2
  s a2 0.6
  s a3 0.2
  s a4 0.5
  s a5 0.5
:
  a1 -> (0.25) a2 + (0.75) a3;
  a2 + a3 + a5 -> a4;
  a4[e0]
-1
