   47   &                                                                               
   48   /exec/                                                                          
   49      begin                                                                        
   50   &  -- Class variables --                                                        
   51      Class1.name:="Class1";                                      & Class (client) 
   52      Class1.n_users:=20;                                                          
   53      Class1.think_t:=2.;                                         & Slice time at c
   54      Class2.name:="Class2";                                      & Class (client) 
   55      Class2.n_users:=50;                                                          
   56      Class2.think_t:=5.;                                         & Slice time at c
   57   &  -- Station variables --                                                      
   58      Class1.DB_t:=0.001;                                                          
   59      Class2.DB_t:=0.002;                                                          
   60      Class1.Disk1_t:=0.005;                                                       
   61      Class2.Disk1_t:=0.005;                                                       
