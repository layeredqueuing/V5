<?xml version="1.0" encoding="utf-8"?>
<lqx-api>
  <info>
    <name>Activity</name>
    <binding>LQNS Bindings</binding>
    <provided-by>LQNS2</provided-by>
  </info>
  <constructors>
    <method>
      <ret>Activity</ret>
      <args>
        <object>task</object>
        <string>name</string>
      </args>
      <name>activity</name>
      <data>Returns an instance of Activity from the current LQNS2 model, whose name corresponds to an activity in the given task.</data>
    </method>
  </constructors>
  <attributes>
    <attr>
      <name>service_time</name>
      <type>double</type>
      <data>Activity service time</data>
    </attr>
    <attr>
      <name>service_time_v</name>
      <type>double</type>
      <data>Activity service time variance</data>
    </attr>
    <attr>
      <name>utilization</name>
      <type>double</type>
      <data>Activity utilization</data>
    </attr>
    <attr>
      <name>proc_waiting</name>
      <type>double</type>
      <data>Activities' processor waiting time</data>
    </attr>
    <attr>
      <name>cv_squared</name>
      <type>double</type>
      <data>The square of the coefficient of variation</data>
    </attr>
    <attr>
      <name>throughput</name>
      <type>double</type>
      <data>The activity throughput</data>
    </attr>
    <attr>
      <name>proc_utilization</name>
      <type>double</type>
      <data>The activities' share of the processor utilization</data>
    </attr>
  </attributes>
</lqx-api>
