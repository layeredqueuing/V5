2 --> [0=>3, 1=>4, 2=>5]
*/

x = array_create(0,1,2);
print("x[0] = ", x[0]);
print("x[1] = ", x[1]);
print("x[2] = ", x[2]);

x[2] = array_create(3,4,5);
